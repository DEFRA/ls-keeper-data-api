{
  "AWS_ACCESS_KEY_ID": "local",
  "AWS_SECRET_ACCESS_KEY": "local",
  "AWS": {
    "ServiceURL": "http://sqs.eu-west-2.localhost.localstack.cloud:4566/"
  },
  "Mongo": {
    "DatabaseUri": "mongodb://127.0.0.1:27017",
    "DatabaseName": "ls-keeper-data-api"
  },
  "DetailedErrors": true,
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u4}] ({Application}/{MachineName}/{ThreadId}/{SourceContext}.{Method}) {Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}
